{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "text"
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "decimal"
    },
    "volume": {
      "type": "string"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "ingredients": {
      "type": "blocks"
    },
    "benefits": {
      "type": "blocks"
    },
    "usages": {
      "type": "blocks"
    },
    "stock": {
      "type": "biginteger"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review"
    },
    "skin_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::skin-type.skin-type"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "isFeatured": {
      "type": "boolean"
    },
    "salesCount": {
      "type": "decimal"
    }
  }
}
